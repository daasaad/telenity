<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0"
	xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="SmsServletJPA">
		<class>httpClientP.SmsServlet_Request</class>
		<properties>
			<property name="eclipselink.jdbc.password" value="bbb" />
			<property name="eclipselink.jdbc.user" value="root" />
			<property name="eclipselink.jdbc.driver" value="com.mysql.jdbc.Driver" />
			<property name="eclipselink.jdbc.url" value="jdbc:mysql://localhost:3306/smsservlet" />
			<property name="eclipselink.ddl-generation" value="create-tables" />
			<property name="eclipselink.target-database" value="MySQL" />
			<property name="eclipselink.logging.level" value="INFO" />
		</properties>
	</persistence-unit>
</persistence>
